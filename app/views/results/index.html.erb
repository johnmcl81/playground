<p id="notice"><%= notice %></p>

<h1>Results</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td>id: <%= result.id %></td>
        <td>level: <%= result.level %></td>
        <td>stage: <%= result.stage %></td>
        <td>result: <%= result.result %></td>
        <td><%= link_to 'Show', result %></td>
        <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
